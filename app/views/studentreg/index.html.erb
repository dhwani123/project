<%- model_class = Studentreg -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:firstname) %></th>
      <th><%= model_class.human_attribute_name(:middlename) %></th>
      <th><%= model_class.human_attribute_name(:lastname) %></th>
      <th><%= model_class.human_attribute_name(:address) %></th>
      <th><%= model_class.human_attribute_name(:dateofbirth) %></th>
      <th><%= model_class.human_attribute_name(:grade) %></th>
      <th><%= model_class.human_attribute_name(:division) %></th>
      <th><%= model_class.human_attribute_name(:gender) %></th>
      <th><%= model_class.human_attribute_name(:contactnumber) %></th>
      <th><%= model_class.human_attribute_name(:bloodgroup) %></th>
      <th><%= model_class.human_attribute_name(:dateofjoining) %></th>
      <th><%= model_class.human_attribute_name(:fname) %></th>
      <th><%= model_class.human_attribute_name(:fqualification) %></th>
      <th><%= model_class.human_attribute_name(:foccupation) %></th>
      <th><%= model_class.human_attribute_name(:fincome) %></th>
      <th><%= model_class.human_attribute_name(:fcontactnumber) %></th>
      <th><%= model_class.human_attribute_name(:femailid) %></th>
      <th><%= model_class.human_attribute_name(:mname) %></th>
      <th><%= model_class.human_attribute_name(:mqualification) %></th>
      <th><%= model_class.human_attribute_name(:moccupation) %></th>
      <th><%= model_class.human_attribute_name(:mincome) %></th>
      <th><%= model_class.human_attribute_name(:mcontactnumber) %></th>
      <th><%= model_class.human_attribute_name(:memailid) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @studentregs.each do |studentreg| %>
      <tr>
        <td><%= link_to studentreg.id, studentreg_path(studentreg) %></td>
        <td><%= studentreg.firstname %></td>
        <td><%= studentreg.middlename %></td>
        <td><%= studentreg.lastname %></td>
        <td><%= studentreg.address %></td>
        <td><%= studentreg.dateofbirth %></td>
        <td><%= studentreg.grade %></td>
        <td><%= studentreg.division %></td>
        <td><%= studentreg.gender %></td>
        <td><%= studentreg.contactnumber %></td>
        <td><%= studentreg.bloodgroup %></td>
        <td><%= studentreg.dateofjoining %></td>
        <td><%= studentreg.fname %></td>
        <td><%= studentreg.fqualification %></td>
        <td><%= studentreg.foccupation %></td>
        <td><%= studentreg.fincome %></td>
        <td><%= studentreg.fcontactnumber %></td>
        <td><%= studentreg.femailid %></td>
        <td><%= studentreg.mname %></td>
        <td><%= studentreg.mqualification %></td>
        <td><%= studentreg.moccupation %></td>
        <td><%= studentreg.mincome %></td>
        <td><%= studentreg.mcontactnumber %></td>
        <td><%= studentreg.memailid %></td>
        <td><%=l studentreg.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_studentreg_path(studentreg), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      studentreg_path(studentreg),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_studentreg_path,
            :class => 'btn btn-primary' %>
